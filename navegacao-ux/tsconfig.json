{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "noEmit": true,
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true
  },
  "include": [
    "**/*.js",
    "**/*.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}